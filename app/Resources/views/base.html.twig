                            <li><a href="#">Zmień dane logowania</a></li>
                            <li role="separator" class="divider"></li>
                            <li><a href="{{ path('fos_user_security_logout') }}">Wyloguj</a></li>
                        </ul>
                    </li>
                    {% endif %}
                    {% if not is_granted('ROLE_USER') %}<li><a href="{{ path('fos_user_security_login') }}">Zaloguj się</a></li>{% endif %}
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="{{ path('fos_user_registration_register') }}">Zarejestruj się</a></li>
                </ul>
            </div>
