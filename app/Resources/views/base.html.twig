                <ul class="nav navbar-nav">
                    <li class="active"><a href="{{ path('homepage') }}">Strona główna</a></li>
                    <li><a href="{{ path('about') }}">O nas</a></li>
                    {% if is_granted('ROLE_USER') %}
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Moje konto <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            {% if is_granted('ROLE_ADMIN') %}<li><a href="#">Panel administracyjny</a></li>{% endif %}
